#' @param palette
#' @param discrete
#'
#' @return
#' @export
#'
#' @examples
scale_color_geopalette = function(palette) {
pal=create_palette(palette = palette, reverse = reverse)
discrete_scale("colour", paste0("geopalette", palette), palette = pal, ...)
}
